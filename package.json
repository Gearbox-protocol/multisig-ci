{
  "name": "@gearbox-protocol/multisig-ci",
  "version": "1.0.0",
  "type": "module",
  "repository": "https://github.com/Gearbox-protocol/multisig-ci.git",
  "license": "BUSL-1.1",
  "scripts": {
    "start": "ts-node -r dotenv/config --esm src/index.ts",
    "verify": "ts-node -r dotenv/config --esm src/verify.ts",
    "act": "act --secret-file .env --input-file act.input",
    "prepare": "husky install",
    "prettier": "prettier --write .",
    "prettier:ci": "npx prettier --check .",
    "lint": "eslint \"**/*.ts\" --fix",
    "lint:ci": "eslint \"**/*.ts\"",
    "typecheck:ci": "tsc --noEmit",
    "test": "vitest run",
    "test:watch": "vitest watch"
  },
  "dependencies": {
    "@actions/core": "^1.10.1",
    "@gearbox-protocol/sdk": "^3.0.0-next.14",
    "@safe-global/api-kit": "^1.3.1",
    "@safe-global/protocol-kit": "^1.3.0",
    "@safe-global/safe-core-sdk-types": "^2.3.0",
    "chalk": "^5.3.0",
    "ethers": "5.7.2",
    "lodash-es": "^4.17.21",
    "p-retry": "^5.1.2",
    "simple-git": "^3.20.0"
  },
  "devDependencies": {
    "@commitlint/cli": "^17.7.2",
    "@commitlint/config-conventional": "^17.7.0",
    "@gearbox-protocol/core-v3": "^1.40.2",
    "@gearbox-protocol/eslint-config": "^1.6.1",
    "@gearbox-protocol/prettier-config": "^1.5.0",
    "@swc/core": "1.3.82",
    "@swc/helpers": "^0.5.2",
    "@types/lodash-es": "^4.17.9",
    "@types/node": "^20.7.1",
    "dotenv": "^16.3.1",
    "eslint": "^8.50.0",
    "husky": "^8.0.3",
    "lint-staged": "^14.0.1",
    "prettier": "^3.0.3",
    "regenerator-runtime": "^0.14.0",
    "ts-node": "^10.9.1",
    "typescript": "^5.2.2",
    "vitest": "^0.34.6"
  },
  "prettier": "@gearbox-protocol/prettier-config",
  "lint-staged": {
    "*.{ts}": [
      "eslint --fix",
      "prettier --write"
    ],
    "*.{json,md,yml}": "prettier --write"
  }
}
