{
  "name": "@gearbox-protocol/multisig-ci",
  "version": "1.0.0",
  "type": "module",
  "repository": "https://github.com/Gearbox-protocol/multisig-ci.git",
  "license": "BUSL-1.1",
  "scripts": {
    "start": "ts-node -r dotenv/config --esm src/index.ts",
    "act": "act --secret-file .env --input-file act.input",
    "prepare": "husky install",
    "prettier": "prettier --write .",
    "prettier:ci": "npx prettier --check .",
    "lint": "eslint \"**/*.ts\" --fix",
    "lint:ci": "eslint \"**/*.ts\"",
    "typecheck:ci": "tsc --noEmit",
    "test": "vitest run",
    "test:watch": "vitest watch"
  },
  "dependencies": {
    "@actions/core": "^1.10.0",
    "@safe-global/api-kit": "^1.1.0",
    "@safe-global/protocol-kit": "^1.0.1",
    "@safe-global/safe-core-sdk-types": "^2.0.0",
    "chalk": "^5.3.0",
    "ethers": "5.7.2",
    "lodash-es": "^4.17.21",
    "p-retry": "^5.1.2"
  },
  "devDependencies": {
    "@commitlint/cli": "^17.6.5",
    "@commitlint/config-conventional": "^17.6.5",
    "@gearbox-protocol/eslint-config": "^1.6.1",
    "@gearbox-protocol/prettier-config": "^1.5.0",
    "@swc/core": "^1.3.64",
    "@swc/helpers": "^0.5.1",
    "@types/lodash-es": "^4.17.7",
    "@types/node": "^20.3.1",
    "dotenv": "^16.1.4",
    "eslint": "^8.42.0",
    "husky": "^8.0.3",
    "lint-staged": "^13.2.2",
    "prettier": "^2.8.8",
    "regenerator-runtime": "^0.13.11",
    "ts-node": "^10.9.1",
    "typescript": "^5.1.3",
    "vitest": "^0.32.2"
  },
  "prettier": "@gearbox-protocol/prettier-config",
  "lint-staged": {
    "*.{ts}": [
      "eslint --fix",
      "prettier --write"
    ],
    "*.{json,md,yml}": "prettier --write"
  }
}
